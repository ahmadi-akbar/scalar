{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "lint:fix": {},
    "build": {
      "outputs": [
        "dist/**",
        "storybook-static/**",
        ".next/**",
        "build/**",
        ".nuxt/**",
        ".output/**",
        "public/build/**",
        "projects/proxy-scalar-com/main"
      ],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "playground": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "playground:modal": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "playground:web": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    }
  }
}
